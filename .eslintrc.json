{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "node": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": ["error", { "vars": "local", "args": "none"}]
    }
}